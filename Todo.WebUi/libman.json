{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    //****************
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery",
      "files": [ "jquery.min.js" ]
    },

    {
      "library": "jquery-validate@1.19.3",
      "destination": "wwwroot/lib/jqueryValidate",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jqueryUnoob",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/fontawesome",
      "files": [ "css/all.min.css", "js/all.min.js" ]
    }

  ]
}